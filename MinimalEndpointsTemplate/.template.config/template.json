{
  "$schema": "http://json.schemastore.org/template",
  "author": "Jos√© Marcelo Fernandes",
  "classifications": ["Web","ASP.NET","MinimalApi"],
  "name": "Minimal Api Endpoints",
  "description": "A project for creating a server with minimal api and the endpoint definition approach.",
  "identity": "MinimalApi.App.1.0",
  "shortName": "minimalApi",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "MinimalEndpointsTemplate",
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net8.0"
        }
      ],
      "defaultValue": "net8.0",
      "replaces": "net8.0"
    },
    "DockerSupport": {
      "type": "parameter",
      "description": "Enables Docker Support",
      "datatype": "bool",
      "defaultValue": "true"
    }
  }, "sources": [
  {
    
    "modifiers": [
      {
        "condition": "(!DockerSupport)",
        "exclude": [
          "docker-compose.yml",
          "Dockerfile",
          ".dockerignore",
          "CustomTemplate.csproj.user"
        ]
      }
    ]
  }
]
}